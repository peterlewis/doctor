{
  "name": "@ptrlws/doctor",
  "version": "1.14.0",
  "description": "Maintain your documentation on SharePoint without pain.",
  "main": "dist/main.js",
  "homepage": "https://github.com/peterlewis/doctor",
  "bin": {
    "doctor": "bin/doctor",
    "doctor_auto": "bin/autocomplete",
    "localm365": "bin/localm365"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run build",
    "preflight": "node scripts/preflight.js",
    "build": "npm run clean && tsc && tsc-alias",
    "watch": "node ./scripts/reset-debug.js && tsc -w",
    "watch:debug": "node ./scripts/init-debug.js && tsc -w",
    "clean": "rm -rf dist mochawesome-report ./cypress/screenshots ./cypress/videos",
    "test": "cypress open",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --browser edge",
    "docs": "cd docs && hogo serve",
    "prepare": "husky install"
  },
  "keywords": [
    "cli",
    "markdown",
    "documentation"
  ],
  "author": "Elio Struyf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estruyf/doctor/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterlewis/doctor.git"
  },
  "maintainers": [
    {
      "name": "Elio Struyf",
      "email": "elio@struyfconsulting.be",
      "url": "https://www.eliostruyf.com"
    }
  ],
  "contributors": [
    {
      "name": "Elio Struyf",
      "email": "elio@struyfconsulting.be",
      "url": "https://www.eliostruyf.com"
    },
    {
      "name": "Peter Lewis",
      "email": "peter@peterlew.is",
      "url": "https://peterlew.is"
    }
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/estruyf"
    },
    {
      "type": "buymeacoffee",
      "url": "https://www.buymeacoffee.com/zMeFRy9"
    }
  ],
  "devDependencies": {
    "@types/clean-css": "4.2.11",
    "@types/cross-spawn": "6.0.6",
    "@types/highlight.js": "9.12.4",
    "@types/inquirer": "9.0.9",
    "@types/listr": "0.14.10",
    "@types/markdown-it": "14.1.2",
    "@types/mime": "3.0.4",
    "@types/omelette": "0.4.5",
    "@types/uuid": "10.0.0",
    "cypress": "15.7.0",
    "husky": "9.1.7",
    "mochawesome": "7.1.4",
    "mochawesome-merge": "5.0.0",
    "mochawesome-report-generator": "6.3.2",
    "node-sp-auth": "3.0.9",
    "tsc-alias": "^1.8.16",
    "tslib": "2.8.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fluentui/svg-icons": "1.1.315",
    "@pnp/cli-microsoft365": "11.1.0",
    "applicationinsights": "3.12.0",
    "arg": "5.0.2",
    "cheerio": "^1.1.2",
    "clean-css": "5.3.3",
    "cross-spawn": "7.0.6",
    "esm": "3.2.25",
    "fast-glob": "3.3.3",
    "gray-matter": "^4.0.3",
    "highlight.js": "11.11.1",
    "html-entities": "2.6.0",
    "inquirer": "13.0.1",
    "kleur": "4.1.5",
    "listr": "0.14.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
